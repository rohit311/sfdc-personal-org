                                        <div class="slds-grid slds-wrap slds-grid--pull-padded"> 
                                            <aura:if isTrue="{!v.conNew.ApplicantType__c != 'Primary'}">
                                                <div class="slds-form-element slds-p-top--x-small slds-p-bottom--x-small slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--3-of-12">
