                                                <div class="slds-form-element__control">
                                                    <lightning:input messageWhenValueMissing="Please Enter Value" required="true" aura:id="resAdd" label="Residence Address Line 1" maxlength="35"  value="{!v.conNew.Address_1__c}" disabled="{!or(v.editFlag,and(and(v.contObj.Address_1__c != null ,v.contObj.Address_1__c != ''),v.dataSource == 'Copy CKYC Data'))}" name="resAdd" placeholder="Enter Residence Address"/>
                                                </div>
