                                                        <option value="" text="--None--" />
                                                        <aura:iteration items="{!v.genderList}" var="gen">
                                                            <option value="{!gen}" text="{!gen}" selected="{!v.conNew.Sex__c == gen}"></option>
                                                        </aura:iteration>
