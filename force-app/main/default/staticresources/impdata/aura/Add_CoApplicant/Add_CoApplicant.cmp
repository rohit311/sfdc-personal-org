                                                <div class="slds-form-element__control">
                                                    <lightning:select aura:id="marStatus" name="marStatus" label="Marital Status" messageWhenValueMissing="Please Select Value" value="{!v.conNew.Marital_Status__c}" required="true" disabled="{!v.editFlag}">
                                                        <option value="" text="--None--" />
                                                        <aura:iteration items="{!v.marStatusList}" var="rel">
                                                            <option value="{!rel}" text="{!rel}" selected="{!v.conNew.Marital_Status__c == rel}"></option>
                                                        </aura:iteration>
                                                    </lightning:select> 
                                                </div>
                                            </div>
                                        </div>
                                    </div>
